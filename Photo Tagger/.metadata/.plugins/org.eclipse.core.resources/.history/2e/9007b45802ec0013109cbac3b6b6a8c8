
//Imports are listed in full to show what's being used
//could just import javax.swing.* and java.awt.* etc..
import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.JComboBox;
import javax.swing.JButton;
import javax.swing.JLabel;
import javax.swing.JList;
import java.awt.BorderLayout;
import java.awt.event.ActionListener;
import java.awt.event.ActionEvent;

public class PhotoTaggerGUI {
	
	public static void main(String[] args) {    
        new PhotoTaggerGUI();
    }

	public PhotoTaggerGUI(){
        
		//Set up frame
		JFrame guiFrame = new JFrame();
		guiFrame.setTitle("Photo Tagger");
		guiFrame.setSize(400, 400);
		guiFrame.setLocationRelativeTo(null);
		
		//Set up disk location
		JLabel nameLabel = new JLabel("Disk Location:");
		nameLabel.setLocation(100, 100);

		
		//Set up  name
		JLabel nameLabel = new JLabel("Name:");
		nameLabel.setLocation(100, 100);
		
		//Set up device name
		JLabel nameLabel = new JLabel("Device Name:");
		nameLabel.setLocation(100, 100);

		//Set up time interval
		JLabel nameLabel = new JLabel("Time Interval:");
		nameLabel.setLocation(100, 100);

		//Set up frame
		guiFrame.add(nameLabel);
		
		
		//make sure the JFrame is visible
        guiFrame.setVisible(true);
	
	}

}
