
public class PhotoTagger {

	//Parameters necessary for getting data from server
	String userName;
	String deviceName;
	String time;
	String date;
	String deltaTime;
	
	public PhotoTagger(){
		
	}
	
	public void setParameters(String time, String date, String userName, String deviceName, String deltaTime){
		
		//Save the parameters
		this.userName = userName;
		this.deviceName = deviceName;
		this.time = time;
		this.date = date;
		this.deltaTime = deltaTime;
		
	}
	
}
