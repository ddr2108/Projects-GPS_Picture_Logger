
public class PhotoTagger {

	//Parameters necessary for getting data from server
	String userName;
	String deviceName;
	String time;
	String date;
	String deltaTime;
	
	public PhotoTagger(){
		
	}
	
	/*
	* setParameters()
	*
	* parameters: 
	* 	String userName - user name of the user
	*	String deviceName - device name of the device
	*	String time - time of the picture
	*	String date - date of the picture
	*	String deltaTime - delta time for GPS coordinates
	* returns: 
	* 	none
	* 
	* saves the parameters needed for connection
	*/
	public void setParameters(String time, String date, String userName, String deviceName, String deltaTime){
		
		//Save the parameters
		this.userName = userName;
		this.deviceName = deviceName;
		this.time = time;
		this.date = date;
		this.deltaTime = deltaTime;
		
	}
	
}
